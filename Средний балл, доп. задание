grades = [[5, 3, 3, 5, 4], [2, 2, 2, 3], [4, 5, 5, 2], [4, 4, 3], [5, 5, 5, 4, 5]]
students = {'Johnny', 'Bilbo', 'Steve', 'Khendrik', 'Aaron'}
students = list(sorted(students))
sum_grades0 = sum(grades[0])/len(grades[0])
sum_grades1 = sum(grades[1])/len(grades[1])
sum_grades2 = sum(grades[2])/len(grades[2])
sum_grades3 = sum(grades[3])/len(grades[3])
sum_grades4 = sum(grades[4])/len(grades[4])
students0 = students[0]
students1 = students[1]
students2 = students[2]
students3 = students[3]
students4 = students[4]
book = {}
book[students0] = sum_grades0
book[students1] = sum_grades1
book[students2] = sum_grades2
book[students3] = sum_grades3
book[students4] = sum_grades4
print(book)
